{
  "name": "crawler",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^18.18.0",
    "sass": "^1.32.0",
    "serve": "^14.2.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.3.0",
    "eslint-config-airbnb": "19.0.4",
    "phaser": "3.60.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-datepicker": "4.21.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "redux": "4.2.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.4.2",
    "vite": "5.0.12",
    "vite-plugin-eslint": "1.8.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "1.2.2",
    "gh-pages": "2.0.1",
    "jest-canvas-mock": "2.5.1",
    "jest-watch-typeahead": "0.6.5",
    "vitest": "1.2.2"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.jsx\"",
    "test": "vitest",
    "test:ci": "vitest --pool=forks",
    "coverage": "vitest run --coverage"
  },
  "eslintIgnore": [
    "src/serviceWorker.jsx"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/{index,reportWebVitals}.{js,jsx}"
    ]
  }
}
